{
	"core": {
		"port": 8080,
		"express": {
			"views": "./server/app/views",
			"engine": "jade",
			"cache": false
		},
		"session": {
			"prefix": "session:",
			"maxAge": 172800000,
			"secret": "default",
			"store": "redis",
			"httpOnly": true,
			"secure": false,
			"path": "/"
		},
		"cookie": {
			"secret": "default"
		}
	}
}
